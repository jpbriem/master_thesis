 for h in hs:\n            hr, hc = h\n            if abs(br-hr) + abs(bc-hc) > 1:\n                # Move the b towards the h\n                dr = hr - br\n                dc = hc - bc\n                if dr!= 0:\n                    new_r = br + dr // abs(dr)\n                else:\n                    new_r = br\n                if dc!= 0:\n                    new_c = bc + dc // abs(dc)\n                else:\n                    new_c = bc\n                board[new_r][new_c] = 'b'\n                board[br][bc] = '.'\n                return True\n\n    # No more movement possible\n    return False\n\n# Test cases\ntests = [\n    [\n        [['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', 'b', 'b', 'b', '.', '.', '.', '.', '.'],\n         ['b', 'b', '.', 'b', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', 'h', 'h', '.', '.', '.', '.'],\n         ['.', '.', '.', 'h', 'h', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.']]\n    ],\n    [\n        [['.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', 'b', 'b', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', 'b', 'b', '.', '.', '.', '.', '.', '.', '.'],\n         ['b', 'b', 'b', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', 'b', 'b', '.', '.', '.', 'h', 'h', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', 'h', 'h', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n         ['.', '.', '.', '.', '.', '.', '.', 