y(grid)\n        newGrid[x][y] = 'f'\n        for dx, dy in product([-1, 0, 1], repeat=2):\n            if dx == 0 and dy == 0:\n                continue\n            if canReplace(x + dx, y + dy) or grid[x + dx][y + dy] == 'f':\n                return True\n        return False\n\n    for i, row in enumerate(grid):\n        for j, cell in enumerate(row):\n            if cell == 'g':\n                if canReplace(i, j):\n                    grid[i][j] = 'f'\n\n# test cases\nfor input_, output in [\n        ([['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', 'g', '.', 'g', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', 'g', '.', 'g', '.', 'g', '.', 'g', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', 'g', '.', 'g', '.', 'g', '.', 'g', '.', 'g', '.', '.', '.']],\n         [['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', 'g', 'f', 'g', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', 'g', 'f', 'g', 'f', 'g', 'f', 'g', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.'],\n          ['.', '.', '.'